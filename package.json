{
  "name": "try-react-ts",
  "version": "1.0.0",
  "scripts": {
    "_start": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.development.js --hot --open",
    "build": "NODE_ENV=production webpack --config config/webpack/webpack.config.production.js",
    "codegen": "apollo client:codegen --target=typescript --useReadOnlyTypes --globalTypesFile=./src/api/graphql/queries/globalTypes.ts",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "start": "run-p _start watch:codegen",
    "test": "jest",
    "gen:pages": "hygen generate pages",
    "watch:codegen": "apollo client:codegen --target=typescript --useReadOnlyTypes --globalTypesFile=./src/api/graphql/queries/globalTypes.ts --watch"
  },
  "lint-staged": {
    "*.graphql": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{tsx}": [
      "eslint --fix",
      "stylelint --fix --config src/.stylelintrc.js",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@types/styled-components": "^4.1.18",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "clsx": "^1.0.4",
    "dotenv": "^8.1.0",
    "formik": "^2.0.1-rc.12",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.2",
    "styled-components": "^4.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.1",
    "@testing-library/react": "^9.1.3",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-transition-group": "^4.2.2",
    "@types/yup": "^0.26.23",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "apollo": "^2.18.0",
    "apollo-codegen": "^0.20.2",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fibers": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "^4.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
