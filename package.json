{
  "name": "try-react-ts",
  "version": "1.0.0",
  "scripts": {
    "_start": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.development.js --hot --open",
    "build": "NODE_ENV=production webpack --config config/webpack/webpack.config.production.js",
    "codegen": "apollo client:codegen --target=typescript --useReadOnlyTypes --globalTypesFile=./src/api/graphql/queries/globalTypes.ts",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "start": "run-p _start watch:codegen",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:codegen": "apollo client:codegen --target=typescript --useReadOnlyTypes --globalTypesFile=./src/api/graphql/queries/globalTypes.ts --watch"
  },
  "lint-staged": {
    "*.graphql": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{tsx}": [
      "eslint --fix",
      "stylelint --fix --config src/.stylelintrc.js",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/graphql": "^14.2.3",
    "@types/node": "^12.7.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-transition-group": "^4.2.1",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "apollo": "^2.17.1",
    "apollo-codegen": "^0.20.2",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "fibers": "^4.0.1",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
